{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**/*",
                "${workspaceFolder}/libs/**/*",
                "${workspaceFolder}/build/Debug/generators"
            ],
            "configurationProvider": "ms-vscode.cmake-tools",
            "defines": [
                "USE_MYSQL=1",
                "USE_POSTGRESQL=1",
                "USE_CPP_YAML=1"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++23",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}